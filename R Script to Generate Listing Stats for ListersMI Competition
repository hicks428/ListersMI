library(dplyr)
library(lubridate)
MyData<-read.csv("MyEBirdData.csv")
MyData<-subset(MyData, select = -c(Submission.ID,
                                   Scientific.Name,
                                   Count,
                                   Location.ID,
                                   Location,
                                   Latitude,
                                   Longitude,
                                   Protocol,
                                   Time,
                                   Duration..Min.,
                                   All.Obs.Reported,
                                   ML.Catalog.Numbers,
                                   Observation.Details,
                                   Checklist.Comments,
                                   Breeding.Code,
                                   Number.of.Observers,
                                   Area.Covered..ha.,
                                   Distance.Traveled..km.)) 
MyData<-subset(MyData, State.Province == "US-AL"| 
                 State.Province == "US-AK" |
                 State.Province == "US-AZ" |
                 State.Province == "US-AR" |
                 State.Province == "US-CA" |
                 State.Province == "US-CO" |
